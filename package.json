		"@vitest/ui": "^4.0.15",
		"fast-check": "^4.3.0",
		"jsdom": "^27.2.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"supabase": "^2.65.6",
		"svelte": "^5.39.5",
		"svelte-check": "^4.3.2",
		"typescript": "^5.9.2",
		"vite": "^7.1.7",
		"vitest": "^4.0.15"
	},
	"dependencies": {
		"@supabase/supabase-js": "^2.80.0",
